@model List<CRUDDemo.ViewModels.UserVM>
@{
    ViewData["Title"] = "Index";
}

<h1>All Users</h1>

@{
    if (String.IsNullOrWhiteSpace(TempData["Sasti"] == null ?   "" : TempData["Sasti"].ToString() )   == false)
    {

        <div class="alert alert-success">  @TempData["Sasti"] </div>
    }
}

<table class="table">
    <tr>
        <td>First name</td>
        <td>Last name</td>
        <td>Phone number</td>
        <td>Email</td>
    </tr>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.PhoneNumber</td>
                <td>@user.Email</td>
            </tr>
        }
    </tbody>
</table>

